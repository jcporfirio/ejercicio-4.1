#include <stdio.h>
#include <stdlib.h>

int main()
{
int num1,num2,mult,div,resto;

printf("Ingrese primer numero entero de 3 digitos: ");
scanf("%d",&num1);
printf("Ingrese segundo numero entero de 3 digitos: ");
scanf("%d",&num2);

mult=num1*num2;
div=num1/num2;
resto=num1%num2;

printf("\nEl producto es: %d",mult);
printf("\nLa division es: %d",div);
printf("\nEl resto es: %d",resto);

}
